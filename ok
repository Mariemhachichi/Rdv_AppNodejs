[32m+[m[32mconst mongoose = require('mongoose')[m
[32m+[m[32mconst bcryptjs =  require('bcryptjs')[m
[32m+[m
[32m+[m[32mconst userSchema = new mongoose.Schema({[m
[32m+[m[32m    name: { type: String, required: true },[m
[32m+[m[32m    email: { type: String, unique: true, required: true },[m
[32m+[m[32m    password: { type: String, required: true },[m
[32m+[m[32m    role: { type: String, enum: ['admin', 'professionel', 'client'], default: 'client' }[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m//Crypter le mot de passe[m
[32m+[m[32muserSchema.pre('save', async function(next) {[m
[32m+[m[32m    if (this.isModified('password')) {[m
[32m+[m[32m        this.password = await bcryptjs.hash(this.password, 10)[m
[32m+[m[32m    }[m
[32m+[m[32m    next()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m//comparer le mot de passe[m
[32m+[m[32muserSchema.methods.comparePassword = async function(password){[m
[32m+[m[32m    return await bcryptjs.compare(password, this.password)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = mongoose.model('User', userSchema)[m
