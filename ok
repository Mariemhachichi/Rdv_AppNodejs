[36m@@ -0,0 +1,25 @@[m
[32m+[m[32m// middleware/auth.js[m
[32m+[m[32mconst jwt = require('jsonwebtoken');[m
[32m+[m[32mconst SECRET_KEY = process.env.SECRET_KEY; // La clÃ© secrÃ¨te utilisÃ©e pour signer le JWT[m
[32m+[m
[32m+[m[32m// Middleware pour vÃ©rifier l'authentification via le JWT[m
[32m+[m[32mconst isAuthenticated = (req, res, next) => {[m
[32m+[m[32m  const token = req.cookies.token; // RÃ©cupÃ©rer le token depuis le cookie[m
[32m+[m
[32m+[m[32m  if (!token) {[m
[32m+[m[32m    return res.status(403).json({ message: 'Token manquant' });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // VÃ©rifier si le token est valide[m
[32m+[m[32m  jwt.verify(token, SECRET_KEY, (err, decoded) => {[m
[32m+[m[32m    if (err) {[m
[32m+[m[32m      return res.status(401).json({ message: 'Token invalide' });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Ajouter les donnÃ©es de l'utilisateur dans la requÃªte pour les utiliser dans la route[m
[32m+[m[32m    req.user = decoded;[m
[32m+[m[32m    next(); // Passer Ã  la prochaine Ã©tape (la route)[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = { isAuthenticated };[m
